{
  "manifest_version": 3,
  "name": "Search History",
  "version": "0.0.1",
  "description": "履歴をインクリメンタルサーチします。",
  "icons": {
    "16": "icon/icon19.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "action": {
    "default_icon": {
      "19": "icon/icon19.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["history"]
}
